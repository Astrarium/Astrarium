        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Foreground" Value="{StaticResource BrushControlBackground}" />
            </Trigger>
        </Style.Triggers>
    </Style>
